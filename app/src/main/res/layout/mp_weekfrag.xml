<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@color/colorSplash" />

    <TextView
        android:id="@+id/mweekT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="113dp"
        android:text="waiting"
        android:textColor="#fffff3"
        android:textSize="18sp" />

    <Button
        android:id="@+id/mw_previous_calendar"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_below="@+id/mwptoday"
        android:layout_marginEnd="-106dp"
        android:layout_toStartOf="@+id/mweekT"
        android:layout_weight="1"
        android:background="@drawable/left"
        android:text="" />

    <TextView
        android:id="@+id/mw_calendar_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/mw_previous_calendar"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="2018년 몇월"
        android:textColor="#fffff3"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/mw_next_calendar"
        android:layout_width="33dp"
        android:layout_height="30dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/mw_previous_calendar"
        android:layout_marginEnd="79dp"
        android:layout_weight="1"
        android:background="@drawable/right"
        android:text="" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/mw_gv_calendar"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="-137dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="일"
            android:textColor="#ff0000"
            android:textSize="14sp" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="월"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="화"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="수"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="목"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="금"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="토"
            android:textColor="#0000ff"
            android:textSize="14sp" />

    </LinearLayout>

    <GridView
        android:id="@+id/mw_gv_calendar"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="137dp"
        android:columnWidth="90dp"
        android:horizontalSpacing="0dp"
        android:numColumns="7"
        android:stretchMode="columnWidth"
        android:verticalSpacing="0dp" />

    <Button
        android:id="@+id/mwptoday"
        android:layout_width="73dp"
        android:layout_height="33dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/wteduri"
        android:text="today"
        android:textColor="#fffff3"
        android:textSize="9sp" />

    <com.github.mikephil.charting.charts.ScatterChart
        android:id="@+id/mwChart"
        android:layout_width="match_parent"
        android:layout_height="293dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/mw_gv_calendar" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="83dp"
        android:text="주 총 시간:"
        android:textColor="#fffff3"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="121dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="84dp"
        android:layout_marginStart="14dp"
        android:text="하루명상시간:"
        android:textColor="@color/black"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="명상"
        android:textColor="#fffff3"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/mp_week"
        android:layout_width="wrap_content"
        android:layout_height="4dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/mw_calendar_title"
        android:layout_marginBottom="98dp"
        android:textColor="@color/black"
        android:textSize="18sp" />

</RelativeLayout>